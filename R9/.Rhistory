install.packages("StatMatch")
data <- iris[, -5] # 因為Species是第五欄位，故移除掉
EUCLI.dist <- dist(data, method="euclidean") # 歐式距離
MANHA.dist <- dist(data, method="manhattan") # 曼哈頓距離
require(StatMatch)
MAHAL.dist <- mahalanobis.dist(data)         # 馬式距離^2
h.cluster <- hclust(EUCLI.dist)
h.cluster <- hclust(MANHA.dist)
h.cluster <- hclust(MAHAL.dist)
